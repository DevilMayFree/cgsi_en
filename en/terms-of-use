                        html_mobile += '<button type="button" onclick="location.href=\'https://www.cgsi.com.my/en/applyonlinenow.jsp\'" class="btn btn-danger btn-lg btn-block">Individual</button>';
                        html_mobile += '</div>';
                        html_mobile += '<div class="form-group dropdown-OpenNewAc-menu-Width2 login-select" style="margin-top: 17px;">';
                        html_mobile += '<button type="button" onclick="location.href=\'https://secure8.itradecimb.com.my/gcCIMBPlus/cliAcctRegEO.jsp?type=C\'" class="btn btn-danger btn-lg btn-block">Corporate</button>';
                        html_mobile += '</div>';
                        html_mobile += '</div>';
                        html_mobile += '</div>';
                        html_mobile += '</div>';
                        html_mobile += '</div>';
                        //create account

                        html_mobile += '<div class="phone-login-menu">';
