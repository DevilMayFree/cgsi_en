        } else if(definecountrycode == "id") {
            $(".desktop-footer-cimbsecurities").html(" PT CGS International Sekuritas Indonesia&nbsp; |&nbsp; No. Izin: KEP-92/PM/1992 & KEP-93/PM/1992" + "<br><img style='width: 100%;' src='https://www.cgs-cimb.co.id/cgscimbresourcesid/system/idfooter.png'>");
        } else if(definecountrycode == "sg") {
            $(".desktop-footer-cimbsecurities").html("Investing in securities and leveraged financial instruments involves market, currency, economic, political, social, business and investment risks. Please ensure you understand the applicable risks involved by reading each product's respective risk warnings contained within this website, as well as the risk disclosure statements in our <a href='https://www.cgs-cimb.com.sg/cgscimbresourcessg/resources/General TermsAndConditions (TnC001-170224).pdf'>General Terms and Conditions</a>, account opening forms and such other documents as we may from time to time prescribe. <br><br>Your use of this website is subject to all applicable laws and regulations and this website shall not be accessed from any jurisdiction where such use is restricted or otherwise prohibited. Please ensure that you read, understand and comply with this website's <a href='terms-of-use'>Terms of Use.</a> <br><br>  CGS International Securities Singapore Pte. Ltd. | Company Reg No: 198701621D");
        } else if(definecountrycode == "th") {
            $(".desktop-footer-cimbsecurities").html(" CGS International Securities (Thailand) Co. Ltd.&nbsp; |&nbsp; Company Reg No.: 0105542081800");
        }  
        
        var modal = $(".cookies-modal")[0];
        var close = $(".cookies-close")[0];
        var accept = $(".cookies-accept")[0];

        if (getCookie("alreayCookiesModal") == "true")
            $(".cookies-modal").hide();

        accept.onclick = function () {
           setCookie("alreayCookiesModal", "true");
            $(".cookies-modal").hide();
        };
        
         close.onclick = function () {
            $(".cookies-modal").hide();
        };
        
        window.onclick = function (event) {
            if (event.target == modal) {
                $(".cookies-modal").hide();
            }
        };
    });
    
    function getFooter() {
        var lang = $("html").attr("lang");
    
        $.ajax({
            type: "GET",
            dataType: 'json',
            url: webservice + "GetFooter.json",
            headers: { "authorization": "webkey" }, 
            data: { lang: lang, footerid: "", type: "", platform: "web" },
            success: function (response) {
                console.log(response);
                if(response.status === "SUCCESS") {
                    var socialhtml = "";
                    var lowermenuhtml = "";
                    var mobilelowermenuhtml = "";
                    var uppermenuhtml = "";
                    var mobileuppermenuhtml = "";
                    $(".footer-lower>*").remove();
                    $(".footer-itemlist-social-frame>*").remove();
                    $(".footer-itemlist-lowermenu-frame>*").remove();
                    
                    //create lowermenutable start
                    var lowermenutable = "";
                    var lowermenutablenum = Math.ceil(response.lowermenu.length/3);
                    
                    lowermenutable += '<table width="100%">';
                    for(var i=0; i<lowermenutablenum; i++) {
                        lowermenutable += '<tr>';
                        
                        if(i == 0)
                            lowermenutable += '<td width="25%" class="footer-itemlist-social-frame" style="vertical-align: top;"></td>';
                        else
                            lowermenutable += '<td width="25%" class="footer-itemlist-social-frame" style="vertical-align: top; visibility: hidden;"></td>';
                        
                        lowermenutable += '<td width="5%">&nbsp;</td>';
                        lowermenutable += '<td width="20%" class="desktop-footer-itemlist-frame lowermenuitem" style="vertical-align: bottom;"></td>';
                        lowermenutable += '<td width="5%">&nbsp;</td>';
                        lowermenutable += '<td width="20%" class="desktop-footer-itemlist-frame lowermenuitem" style="vertical-align: bottom;"></td>';
                        lowermenutable += '<td width="5%">&nbsp;</td>';
                        lowermenutable += '<td width="20%" class="desktop-footer-itemlist-frame lowermenuitem" style="vertical-align: bottom;"></td>';
                        lowermenutable += '</tr>';
                    }
                    lowermenutable += '</table>';
                    $(".footer-lower").append(lowermenutable);
                    //create lowermenutable end
                    
                    //socialhtml start
                    for(var i=0; i<response.social.length;i++) {
                        socialhtml += '<a href="' + response.social[i].hyperlink + '" target="_blank">';
                        socialhtml += '<img src="' + response.social[i].background + '" class="footer-icon"></a>';
                    }
                    $(".footer-itemlist-social-frame").append(socialhtml);
                    //socialhtml end
                    
                    //lowermenuhtml start
                    for(var i=0; i<response.lowermenu.length;i++) {
                        lowermenuhtml = '';
                        lowermenuhtml += '<a href="' + response.lowermenu[i].hyperlink + '" target="_blank" style="color: #ffffff;">';
                        lowermenuhtml += '<p class="footer-btn">' + response.lowermenu[i].content  + '</p></a>';
                        $(".lowermenuitem").eq(i).append(lowermenuhtml);
                    }
                    //lowermenuhtml end
                    
                    //mobilelowermenuhtml start
                    for(var i=0; i<response.lowermenu.length;i++) {
                        mobilelowermenuhtml += '<p class="footer-btn" onclick="javascript: window.open(\'' + response.lowermenu[i].hyperlink + '\', \'_blank\');">';
                        mobilelowermenuhtml += response.lowermenu[i].content;
                        mobilelowermenuhtml += '</p>';
                    }
                    $(".footer-itemlist-lowermenu-frame").append(mobilelowermenuhtml);
                    //mobilelowermenuhtml end
                    
                    //uppermenuhtml start
                    var item = 0;
                    var uppermenuitemindex = 0;
                    var uppermenuitemgroupnum = Math.ceil(response.uppermenu.length/3);
                    for(var i=0; i<response.uppermenu.length;i++) {
                        uppermenuhtml += '<a href="' + response.uppermenu[i].hyperlink + '" style="color: #ffffff;">';
                        uppermenuhtml += '<p class="footer-btn">' + response.uppermenu[i].content  + '</p></a>';
                        
                        item ++;
                        if(item == uppermenuitemgroupnum || i+1 == response.uppermenu.length) {
                            $(".uppermenuitem").eq(uppermenuitemindex).append(uppermenuhtml);
                            uppermenuhtml = '';
                            item = 0;
                            uppermenuitemindex++;
                        }
                    }
                    //uppermenuhtml end
                    
                    //mobileuppermenuhtml start
                    var mobileitem = 0;
                    var mobileuppermenuitemindex = 0;
                    var mobileuppermenuitemgroupnum = Math.ceil(response.uppermenu.length/2);
                    for(var i=0; i<response.uppermenu.length;i++) {
                        mobileuppermenuhtml += '<a href="' + response.uppermenu[i].hyperlink + '" style="color: #ffffff;">';
                        mobileuppermenuhtml += '<p class="footer-btn">' + response.uppermenu[i].content  + '</p></a>';
                        
                        mobileitem ++;
                        if(mobileitem == mobileuppermenuitemgroupnum || i+1 == response.uppermenu.length) {
                            $(".mobile-uppermenuitem").eq(mobileuppermenuitemindex).append(mobileuppermenuhtml);
                            mobileuppermenuhtml = '';
                            mobileitem = 0;
                            mobileuppermenuitemindex++;
                        }
                    }
                    //mobileuppermenuhtml end
                } else {
                    checkSessionExpired(response.replymsg);
                    $("#errorModal>div>div>div.modal-body").html(response.replymsg);
                    $("#errorModal").modal();
                }
            },
            error: function(XMLHttpRequest) {

            },
            complete: function () {
                getLanguage();
            }
        });
    }
</script>

    </body>
</html>

