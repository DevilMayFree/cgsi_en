                        html_mobile += '<div class="form-group dropdown-OpenNewAc-menu-Width2 login-select" style="margin-top: 17px;">';
                        html_mobile += '<button type="button" onclick="location.href=\'https://secure8.itradecimb.com.my\'" class="btn btn-danger btn-lg btn-block">CGS International iTrade</button>';
                        html_mobile += '</div>';
                        html_mobile += '<div class="form-group dropdown-OpenNewAc-menu-Width2 login-select" style="margin-top: 17px;">';
                        html_mobile += '<button type="button" onclick="location.href=\'https://www.eipocimb.com/\'" class="btn btn-danger btn-lg btn-block">eIPO</button>';
                        html_mobile += '</div>';
                        html_mobile += '<div class="form-group dropdown-OpenNewAc-menu-Width2 login-select" style="margin-top: 17px;">';
                        html_mobile += '<button type="button" onclick="location.href=\'https://login.iress.com.sg/Logon/CGSCIMBCFDMY\'" class="btn btn-danger btn-lg btn-block">IRESS</button>';
                        html_mobile += '</div>';
                        html_mobile += '</div>';
                        html_mobile += '<div class="phone-login-menu strike">';
                        html_mobile += '<span>or</span>';
                        html_mobile += '</div>';
                        html_mobile += '<div class="phone-login-menu" style="padding:30px;">';
                        html_mobile += '<button type="button" class="btn btn-outline-danger btn-lg btn-block phone-btn-block phone-menu-btn-CreateAc" data-i18n="[html]header.label_createaccountuppercase"></button>';
                        html_mobile += '</div>';
                        html_mobile += '</div>';
                        html_mobile += '</div>';
                        html_mobile += '</ul>';
                        html_mobile += '</div>';
                    } else {
                        html_mobile += '<div class="btn-group phone-menu-btn-group-height" role="group" aria-label="Basic example">';
                        html_mobile += '<button type="button" class="btn phone-menu-btn-Login" data-i18n="[html]header.label_loginuppercase"></button>';
                        html_mobile += '<button type="button" class="btn phone-menu-btn-CreateAc" onclick="javascript: location.href=\'openanaccount\'" data-i18n="[html]header.label_createaccountuppercase"></button>';
                        html_mobile += '</div>';
                        html_mobile += '<div class="phone-login-menu">';
                        html_mobile += '<div style="padding: 0px 30px 9px 30px; ">';
                        html_mobile += '<button type="button" class="phone-login-menu close" data-dismiss="alert" aria-label="Close">';
                        html_mobile += '<span aria-hidden="true">&times;</span>';
                        html_mobile += '</button>';
                        html_mobile += '<a style="font-size: 22px;font-weight: 900;"><b data-i18n="[html]header.label_memberlogin"></b></a>';
                        html_mobile += '</div>';
                        html_mobile += '<div class="phone-login-menu menu-select-style"> ';
                        html_mobile += '<div class="phone-select2 phone-login-menu form-group dropdown-OpenNewAc-menu-Width2 menu-select-padding login-select">';
                        html_mobile += '<select class="phone-select2-menu">';
                        html_mobile += '<option value="0" data-i18n="[html]header.option_selectproduct"></option>';
                        html_mobile += '</select>';
                        html_mobile += '</div>';
                        html_mobile += '<div class="phone-select3 phone-login-menu form-group dropdown-OpenNewAc-menu-Width3 menu-select-padding login-select">';
                        html_mobile += '<select class="phone-select3-menu">';
                        html_mobile += '<option value="0" data-i18n="[html]header.option_selectplatform"></option>';
                        html_mobile += '</select>';
                        html_mobile += '</div>';
                        html_mobile += '<div class="phone-login-menu" style="padding:30px;">';
                        html_mobile += '<button type="button" disabled="disabled" class="btn btn-danger btn-lg btn-block phone-btn-block phone-login-btn" data-i18n="[html]header.btn_proceed"></button>';
                        html_mobile += '</div>';
                        html_mobile += '<div class="phone-login-menu strike">';
                        html_mobile += '<span>or</span>';
                        html_mobile += '</div>';
                        html_mobile += '<div class="phone-login-menu" style="padding:30px;">';
                        html_mobile += '<button type="button" class="btn btn-outline-danger btn-lg btn-block phone-btn-block" onclick="javascript: location.href=\'openanaccount\'" data-i18n="[html]header.label_createaccountuppercase"></button>';
                        html_mobile += '</div>';
                        html_mobile += '</div>';
                        html_mobile += '</div>';
                        html_mobile += '</ul>';
                        html_mobile += '</div>';
                    }

                    $(".navbar.navbar-expand-md.navbar-light.bg-white.no-padding.muli-font-family").html(html_mobile);
                    mobileMenuHandler(response);
                    $(".login-select>.select-selected").remove();
                    $(".login-select>.select-items").remove();
                    selector();
                    //mobile version end

                    //language selector start
                    setMenuHeader();
                    //language selector end

                    $(".navbar-brand.destop-navbar-brand").attr("href", homepage_obj.page_name);
                    $(".navbar-brand.mobile-navbar-brand").attr("href", homepage_obj.page_name);
                    $(".nav.collapse.navbar-collapse>li>a").eq(0).attr("href", homepage_obj.page_name);
                    //active menu start
                    var currentPageWithParm = window.location.href.split("/")[window.location.href.split("/").length - 1];
                    var currentPage = currentPageWithParm.split(".jsp")[0];
                    var selectedChildid;
                    var page_name_active = "";
                    for (var i = 0; i < response.menu.length; i++) {
                        if (response.menu[i].page_name.indexOf(currentPage) > -1) {
                            var targetPageWithParm = response.menu[i].page_name;
                            var targetPageWithJsp = targetPageWithParm.split(".jsp")[0];
                            if (targetPageWithJsp == currentPage) {
                                selectedChildid = response.menu[i].child_id;
                                page_name_active = response.menu[i].menu_id;
                            }
                        }
                    }

                    if (page_name_active != "" && page_name_active != "1") {
                        while (selectedChildid != "1") {
                            for (var i = 0; i < response.menu.length; i++) {
                                if (response.menu[i].menu_id == selectedChildid) {
                                    selectedChildid = response.menu[i].child_id;
                                    page_name_active = response.menu[i].menu_id;
                                }
                            }
                        }

                        $(".nav-item.dropdown.dropdown-menu-hover.dropdown-crow>a.nav-link[menuid='" + page_name_active + "']").addClass("active");
                    }

                    if ($(".header_sub[href='" + currentPage + "']").length > 0)
                        $(".header_sub[href='" + currentPage + "']").addClass("active");
                    $(".header_sub[href='home']").addClass("active");
                    //active menu end

                    //outstandspace start
                    outstandspaceWithOutMenu();
                    $('button.navbar-toggler.phone-btn-nav').click(function () {
                        if (!$("#navbarTogglerDemo02").hasClass("show")) {
                            $("#navbarTogglerDemo02").removeAttr("style");
                            $("body").append("<mobiletempbody></mobiletempbody>")
                            $("body>*:not('.sticky-top')").appendTo("mobiletempbody");
                            $("#iframe-alertbar").hide();
                            $(".navbar-toggler.phone-btn-nav");
                            $("button.navbar-toggler.phone-btn-nav>.navbar-toggler-icon").attr("style", "height: 16px; background-image: url('https://www.cgsi.com/cgsiresources/system/menucancel.svg')");
                        } else {
                            $("mobiletempbody>*").appendTo("body");
                            $("mobiletempbody").remove();
                            $("#iframe-alertbar").show();
                            $("button.navbar-toggler.phone-btn-nav>.navbar-toggler-icon").removeAttr("style");
                        }
                    });
                    $('#navbarTogglerDemo02').on('shown.bs.collapse', function () {
                        outstandspaceWithMenu();
                    });
                    //outstandspace end

                    //submenu controll start
                    var submenu = $(".dropdown-submenu.destop-dropdown-submenu");
                    submenu.on("click", function () {
                        $("div.dropdown-menu").hide();
                        $(this).children(".dropdown-menu").show();
                        $(".dropdown-menu.product-and-serivces-dropdown-menu-style").mouseleave(function () {
                            $("div.dropdown-menu").hide();
                        });
                    });
                    //submenu controll end

                    memberloginmenu(response);
                } else {
                    checkSessionExpired(response.replymsg);
                    $("#errorModal>div>div>div.modal-body").html(response.replymsg);
                    $("#errorModal").modal();
                }

            },
            error: function (XMLHttpRequest) {

            },
            complete: function () {
                getLanguage();
                $(".se-pre-con").fadeOut("fast");
            }
        });
    }

    function mobileMenuHandler(response) {
        resetMobileList();
        $(".nav-item.item-style.phone-nav-item.phone-nav-item-margin").show();
        $(".menu.nav-link.phone-nav-link").parent().show();
        // $(".mobile-menu-footer").show();
        // $(".btn-group.phone-menu-btn-group-height").show();
        //back button
        for (var i = 0; i < response.menu.length; i++) {
            //menu
            if (response.menu[i].child_id == "1") {
                var currentmenu_num = i;
                //menu -> submenu
                $(".menu.nav-link.phone-nav-link.menu" + currentmenu_num).parent().click(function (event) {
                    resetMobileList();
                    var menunumber = $(event.target).attr("menunumber");
                    $(".nav-link.phone-nav-link.phone-back-btn").parent().show();
                    $(".nav-link.phone-nav-link.menu" + menunumber + "-submenu").parent().show();
                });
                var currentmenu_id = response.menu[i].menu_id;
                ;
                for (var j = 0; j < response.menu.length; j++) {
                    if (response.menu[j].child_id == currentmenu_id) {
                        var currentsubmenu_id = response.menu[j].menu_id;
                        for (var k = 0; k < response.menu.length; k++) {
                            if (response.menu[k].child_id == currentsubmenu_id) {

                                //submenu -> subsubmenu
                                $(".nav-link.phone-nav-link.menu" + currentmenu_num + "-submenu").parent().click(function (event) {
                                    resetMobileList();
                                    var menunumber = $(event.target).attr("menunumber");
                                    var submenunumber = $(event.target).attr("submenunumber");
                                    $(".nav-link.phone-nav-link.menu" + menunumber + "-item-subitem-phone-back-btn").parent().show();
                                    $(".nav-link.phone-nav-link.menu" + menunumber + "-subsubmenu.subsubmenu" + submenunumber).parent().show();
                                });
                                //subsubmenu -> submenu
                                $(".nav-link.phone-nav-link.menu" + currentmenu_num + "-item-subitem-phone-back-btn").parent().click(function () {
                                    resetMobileList();
                                    var menunumber = $(event.target).attr("menunumber");
                                    $(".nav-link.phone-nav-link.phone-back-btn").parent().show();
                                    $(".nav-link.phone-nav-link.menu" + menunumber + "-submenu").parent().show();
                                });
                            }
                        }
                    }
                }
            }
        }

        //submenu -> menu
        $(".nav-link.phone-nav-link.phone-back-btn").parent().click(function () {
            resetMobileList();
            $(".nav-item.item-style.phone-nav-item.phone-nav-item-margin").show();
            $(".menu.nav-link.phone-nav-link").parent().show();
            // $(".mobile-menu-footer").show();
            // $(".btn-group.phone-menu-btn-group-height").show();
        });
        $(".phone-menu-btn-Login").click(function () {
            $("#navbarTogglerDemo02>ul>*").hide();
            $(".phone-login-menu").show();
        });
        $(".phone-login-menu.close").click(function () {
            $("#navbarTogglerDemo02>ul>*").show();
            resetMobileList();
            $(".nav-item.item-style.phone-nav-item.phone-nav-item-margin").show();
            $(".menu.nav-link.phone-nav-link").parent().show();
            // $(".mobile-menu-footer").show();
            // $(".btn-group.phone-menu-btn-group-height").show();
        });
        $(".phone-menu-btn-CreateAc").click(function () {
            $("#navbarTogglerDemo02>ul>*").hide();
            $(".phone-createaccount-menu").show();
        });
        $(".phone-createaccount-menu.close").click(function () {
            $("#navbarTogglerDemo02>ul>*").show();
            resetMobileList();
            $(".nav-item.item-style.phone-nav-item.phone-nav-item-margin").show();
            $(".menu.nav-link.phone-nav-link").parent().show();
            // $(".mobile-menu-footer").show();
            // $(".btn-group.phone-menu-btn-group-height").show();
        });
    }

    function resetMobileList() {
        $(".nav-item.item-style.phone-nav-item.phone-nav-item-margin").hide();
        $(".btn_back.nav-link.phone-nav-link").parent().hide();
        $(".menu.nav-link.phone-nav-link").parent().hide();
        $(".submenu.nav-link.phone-nav-link").parent().hide();
        $(".subsubmenu.nav-link.phone-nav-link").parent().hide();
        $(".phone-createaccount-menu").hide();
        $(".phone-login-menu").hide();
        $(".mobile-menu-footer").hide();
        $(".btn-group.phone-menu-btn-group-height").hide();
    }

    function memberloginmenu(response) {
        var opennewac2html = "";
        for (var i = 0; i < response.memberlogina.length; i++) {
            opennewac2html += '<option value="' + response.memberlogina[i].memberloginaid + '">' + response.memberlogina[i].choice + '</option>';
        }

        $(".dropdown-OpenNewAc-menu-Width2.login-select>select").append(opennewac2html);
        $('.dropdown-OpenNewAc-menu-Width1.login-select').on('change', function () {
            $('.login-btn').removeAttr('disabled');
            $('.phone-login-btn').removeAttr('disabled');
        });
        $('.dropdown-OpenNewAc-menu-Width2.login-select').on('change', function () {
            var memberloginaid = $(this).find("select").val();
            var lang = $("html").attr("lang");
            var opennewac3html = "";
            $.ajax({
                type: "POST",
                dataType: 'json',
                url: webservice + "GetMemberLoginb",
                headers: {"authorization": "webkey"},
                data: {memberloginaid: memberloginaid, memberloginbid: "", lang: lang, platform: "web"},
                success: function (response) {
                    if (response.text.length > 1) {
                        opennewac3html += '<option value="0" data-i18n="[html]header.option_selectplatform"></option>';
                    }

                    for (var i = 0; i < response.text.length; i++) {
                        opennewac3html += '<option value="' + response.text[i].hyperlink + '">' + response.text[i].choice + '</option>';
                    }

                    $(".dropdown-OpenNewAc-menu-Width3.login-select>select>*").remove();
                    $(".dropdown-OpenNewAc-menu-Width3.login-select>select").append(opennewac3html);
                    $('.login-btn').removeAttr('disabled');
                    $('.phone-login-btn').removeAttr('disabled');
                    selector();
                },
                error: function (XMLHttpRequest) {

                }
            });
        });
        $('.login-btn, .phone-login-btn').on('click', function () {
            var platformSelector = $('.platform-select').val();
            window.open(platformSelector, '_blank');
        });
    }

    function resetSelector() {
        $(".dropdown-OpenNewAc-menu-Width>select").prepend('<option value="#" data-i18n="[html]global.option_' + cimbwebsitezonelangtitlea.toLowerCase() + '"></option>');
        $(".country-select-dropdown>option").eq(0).attr("selected", true);
        $('.nav-link.right-nav-link,.phone-menu-btn-Login').on('click', function () {
            $('.form-group.dropdown-OpenNewAc-menu-Width3 .select-items').removeClass('select-disable');
            $('.form-group.dropdown-OpenNewAc-menu-Width3 .select-selected').removeClass('select-disable-filter');
            $('.form-group.dropdown-OpenNewAc-menu-Width1 .select-items').addClass('select-hide');
            $('.form-group.dropdown-OpenNewAc-menu-Width2 .select-items').addClass('select-hide');
            $('.form-group.dropdown-OpenNewAc-menu-Width3 .select-items').addClass('select-hide');
            $('.country-select').val(0);
            $('.platform-select').val(0);
            $('.product-select').val(0);
            $('.form-group.dropdown-OpenNewAc-menu-Width1 .select-selected').text(i18n.t("header.option_selectcountry"));
            $('.form-group.dropdown-OpenNewAc-menu-Width2 .select-selected').text(i18n.t("header.option_selectproduct"));
            $('.form-group.dropdown-OpenNewAc-menu-Width3 .select-selected').text(i18n.t("header.option_selectplatform"));
            $('.login-btn').attr("disabled", "disabled");
            $('.phone-login-btn').attr("disabled", "disabled");
        });
    }

    function changeMenubar() {
        var prevScrollpos = window.pageYOffset;
        var mainMenuBar = $(".navbar.navbar-expand-sm.navbar-light.navbar-padding.nav-border-bottom-gray.muli-font-family.navbar-white-font");
        window.onscroll = function () {
            var currentScrollPos = window.pageYOffset;
            if ($(window).width() > 768) {
                if (prevScrollpos > currentScrollPos) {
                    $(".bluenav")[0].style.visibility = "visible";
                    $(".bluenav")[0].style.top = "0";
                    mainMenuBar[0].style.top = "0";
                    $(".sticky-sidebar").removeClass("up");
                } else {
                    $(".bluenav")[0].style.visibility = "hidden";
                    $(".bluenav")[0].style.top = "-50px";
                    mainMenuBar[0].style.top = "-" + $(".bluenav").height() + "px";
                    $(".sticky-sidebar").addClass("up");
                }
                prevScrollpos = currentScrollPos;
            }
        };
    }

    $("#topbutton").on("click touch touchstart", function () {
        topFunction();
    });
</script>
        
