<script>
    var insightsMenuResponse;
    var alertbarSlider;
    $(document).ready(function () {
        //dev start
        if (definecountrycode == "my") {
            $(".dev-needed").show();
            $(".basic-needed").remove();
        } else if (definecountrycode == "international" || definecountrycode == "sg") {
            $(".intlsg-needed").show();
            $(".basic-needed").remove();
        } else {
            $(".dev-needed").remove();
        }
        //dev end
        GetHomePageHeader();
        resetSelector();
        changeMenubar();
        $(window).resize(function () {
            if (!$(".phone-login-menu").is(":visible")) {
                if ($("#navbarTogglerDemo02").is(":visible")) {
                    if ($(".mobile-menu-footer").is(":visible")) {
                        $(".mobile-menu-footer").removeAttr("style");
                        outstandspaceWithMenu();
                    } else {
                        $(".mobile-menu-footer").removeAttr("style");
                        outstandspaceWithMenu();
                        $(".mobile-menu-footer").hide();
                    }
                } else {
                    $(".mobile-menu-footer").removeAttr("style");
                    outstandspaceWithOutMenu();
                }
            }
        });
    });
    function outstandspaceWithOutMenu() {
        var outstandspace = 20 + $(".mobile-menu-footer").actual('outerHeight') + $(window).height() - ($("nav.navbar.navbar-expand-md.navbar-light.bg-white.no-padding.muli-font-family").outerHeight() + $("#navbarTogglerDemo02").actual('outerHeight')) - 1;
        if (outstandspace > (20 + $(".mobile-menu-footer").actual('outerHeight')))
            $(".mobile-menu-footer").height(outstandspace);
    }

    function outstandspaceWithMenu() {
        var outstandspace = $(".mobile-menu-footer").outerHeight() + $(window).height() - $("nav.navbar.navbar-expand-md.navbar-light.bg-white.no-padding.muli-font-family").outerHeight() - 1;
        if (outstandspace > $(".mobile-menu-footer").actual('outerHeight'))
            $(".mobile-menu-footer").height(outstandspace);
    }

    function setCookie(cname, cvalue, exdays) {
        var d = new Date();
        d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
        var expires = "expires=" + d.toUTCString();
        document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
    }

    function getCookie(cname) {
        var name = cname + "=";
        var ca = document.cookie.split(';');
        for (var i = 0; i < ca.length; i++) {
            var c = ca[i];
            while (c.charAt(0) == ' ') {
                c = c.substring(1);
            }
            if (c.indexOf(name) == 0) {
                return c.substring(name.length, c.length);
            }
        }
        return "";
    }

    function alertBarClose(value) {
        setCookie("alertbar|" + definecountrycode + "|" + $("html").attr("lang"), value);
        $("#iframe-alertbar>.cimb-notification").hide();
        clearInterval(alertbarSlider);
    }

    function getCurrentDateInTitle() {
        var today = new Date();
        var date = today.getDate();
        var month = today.getMonth();
        var year = today.getFullYear();
        if (date < 10)
            date = '0' + date;
        var monthNames = ["January", "February", "March", "April", "May", "June", "July", "August", "September", "October", "November", "December"];
        return date + " " + monthNames[month] + " " + year;
    }

    function GetHomePageHeader() {
        var lang = $("html").attr("lang");
        var pagename = $("body").attr("page") || "";
        $.ajax({
            type: "POST",
            dataType: 'json',
            url: webservice + "GetHomePageHeader.json",
            headers: {"authorization": "webkey"},
            data: {lang: lang, pagename: pagename},
            success: function (response) {
                if (response.status === "SUCCESS") {
                    if (response.isactive == 0)
                        location.href = "error.jsp";
                    var html_desktop = "";
                    var homepage_obj;
                    //alertbar start
                    var html = "";
                    if (getCookie("alertbar|" + definecountrycode + "|" + $("html").attr("lang")) != getCurrentDateInTitle()) {
                        for (var i = 0; i < response.alertbar.length; i++) {
                            if (i == 0) {
                                html += '<div class="cimb-notification" id="' + i + '">';
                            } else {
                                html += '<div class="cimb-notification" id="' + i + '" style="display: none;">';
                            }

                            html += '<span class="closebtn" onclick="alertBarClose(\'' + getCurrentDateInTitle() + '\')">&times;</span>';
                            html += response.alertbar[i].message;
                            html += '</div>';
                        }

                        $("#iframe-alertbar").append(html);
                        alertbarSlider = setInterval(function () {
                            var currentid = $(".cimb-notification:visible").attr("id");
                            var nextid = parseInt(currentid) + 1;
                            if ($(".cimb-notification[id=" + nextid + "]").length == 1) {
                                $(".cimb-notification").hide();
                                $(".cimb-notification[id=" + nextid + "]").show();
                            } else if (currentid != 0) {
                                $(".cimb-notification").hide();
                                $(".cimb-notification[id=0]").show();
                            }

                        }, 10000);
                    }
                    //alertbar end

                    //desktop version start
                    for (var i = 0; i < response.menu.length; i++) {
                        if (response.menu[i].menu_id == "1")
                            homepage_obj = response.menu[i];
                        if (response.menu[i].child_id == "1") {
                            //html_desktop += html_desktop code'

                            var isFirstTime = false;
                            var submenuFirstTime = false;
                            var haveChild = false;
                            var haveSubSubmenu = false;
                            var currentmenu_id = response.menu[i].menu_id;
                            html_desktop += '<li class="nav-item dropdown dropdown-menu-hover dropdown-crow">';
                            if (response.menu[i].submenu_style == "0") {
                                //child menu
                                for (var j = 0; j < response.menu.length; j++) {
                                    if (response.menu[j].child_id == currentmenu_id) {
                                        haveChild = true;
                                        if (isFirstTime == false) {
                                            html_desktop += '<ul class="dropdown-menu product-and-serivces-dropdown-menu-style" aria-labelledby="navbarDropdownMenuLink">';
                                            isFirstTime = true;
                                        }

                                        html_desktop += '<li class="dropdown-submenu destop-dropdown-submenu style0" style="cursor: pointer;">';
                                        var submenu_id = response.menu[j].menu_id;
                                        for (var k = 0; k < response.menu.length; k++) {
                                            if (response.menu[k].child_id == submenu_id) {
                                                haveSubSubmenu = true;
                                                if (submenuFirstTime == false) {

                                                    html_desktop += '<div class="dropdown-menu"id="style-4" style="width:257px; padding-top: 0px;">';
                                                    html_desktop += '<table width="100%">';
                                                    submenuFirstTime = true;
                                                }
                                                var subsubmenu_link = response.menu[k].page_name;
                                                if (subsubmenu_link == undefined || subsubmenu_link == "") {
                                                    html_desktop += '<tr class="sub-menu-border-bottom" style="cursor: pointer;"><td>';
                                                    html_desktop += '<a class="dropdown-item no-bottom-padding-dropdown-item" style="white-space:inherit !important; max-width:250px;">' + response.menu[k].menu_name + '</a>';
                                                } else {
                                                    html_desktop += '<tr class="sub-menu-border-bottom" style="cursor: pointer;" ' + getMenuOnClickLocation(subsubmenu_link) + '><td>';
                                                    html_desktop += '<a ' + getMenuHref(subsubmenu_link) + ' class="dropdown-item no-bottom-padding-dropdown-item" style=" color: black; white-space:inherit !important; max-width:250px;">' + response.menu[k].menu_name + '</a>';
                                                }

                                                var desc = "";
                                                if (response.menu[k].description != undefined && response.menu[k].description != "") {
                                                    html_desktop += '<a ' + getMenuHref(subsubmenu_link) + ' style="font-weight: normal !important"><div class="dropdown-item-desc" style="margin-top: 10px;">' + response.menu[k].description + '</div></a></td></tr>';
                                                } else {
                                                    html_desktop += '<a ' + getMenuHref(subsubmenu_link) + ' style="font-weight: normal !important"><div class="dropdown-item-desc">' + desc + '</div></a></td></tr>';
                                                }
                                            }
                                        }

                                        if (submenuFirstTime == true) {
                                            html_desktop += '</table>';
                                            html_desktop += '</div>';
                                            submenuFirstTime = false;
                                        }
                                        var submenu_link = response.menu[j].page_name;
                                        if (submenu_link == undefined || submenu_link == "") {
                                            if (haveSubSubmenu == false) {
                                                html_desktop += '<a class="dropdown-item sub_menu_arrow_right" style="white-space:normal !important; color:black;">' + response.menu[j].menu_name + '</a>';
                                            } else {
                                                html_desktop += '<a class="dropdown-item sub_menu_arrow_right" style="white-space: inherit;">' + response.menu[j].menu_name + '</a>';
                                            }
                                        } else {
                                            if (haveSubSubmenu == false) {
